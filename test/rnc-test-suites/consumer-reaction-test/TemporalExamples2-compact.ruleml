<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="../../../relaxng/drivers/consumer-reaction.rnc" type="application/relax-ng-compact-syntax"?>
<Rule xmlns="http://ruleml.org/spec" xmlns:xs="http://www.w3.org/2001/XMLSchema"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dc="http://purl.org/dc/elements/1.1"
  xmlns:owltime="http://www.w3.org/2006/time"
  xmlns:this="http://consumer.ruleml.org/exa/consumer-reaction/TemporalExample2#"
  xmlns:ex="http://reaction.ruleml.org/exa/dr/TemporalExample2#"
  material="yes" closure="universal">
  <if>
    <!-- rule body using different times-->
    <And>

      <!-- time -->
        <Time>
            <Ind>t1</Ind>
        </Time>
        <Time>
            <Var>x</Var>
        </Time>
        <Time key="this:time1">
            <Data xsi:type="xs:dateTime">2011-03-21T10:00:00-04:00</Data>
        </Time>
        <Time>
          <!--  slotted -->
          <slot>
            <Ind>year</Ind>
            <Var>Y</Var>
          </slot>
          <slot>
            <Ind>month</Ind>
            <Var>M</Var>
          </slot>
          <slot>
            <Ind>day</Ind>
            <Var>D</Var>
          </slot>
        </Time>
        <Time>
          <!-- frame representation -->
          <oid>
            <Var type="ex:DatetimeClass1">X</Var>
          </oid>
          <slot>
            <Ind>date</Ind>
            <Var>Date</Var>
          </slot>
          <slot>
            <Ind>time</Ind>
            <Var>Time</Var>
          </slot>
        </Time>
        <Time>
          <!-- frame representation -->
          <oid>
            <Ind type="ex:DatetimeClass2">datetime2</Ind>
          </oid>
          <slot>
            <Ind>datetime</Ind>
            <Var>DT</Var>
          </slot>
        </Time>
        <Time>
          <!-- frame representation -->
          <oid>
            <Ind type="ex:TimeIntervalClass">interval1</Ind>
          </oid>
          <slot>
            <Ind>start</Ind>
            <Time>
              <arg>
                <Var>T1</Var>
              </arg>
            </Time>
          </slot>
          <slot>
            <Ind>end</Ind>
            <Time>
              <arg>
                <Var>T2</Var>
              </arg>
            </Time>
          </slot>
        </Time>
        <Time>
          <!-- after(2 years) -> Y -->
          <slot>
            <Time>
              <arg>
                <After>
                  <arg>
                    <Data xsi:type="xs:duration">P2Y</Data>
                  </arg>
                </After>
              </arg>
            </Time>
            <Var>Y</Var>
          </slot>
        </Time>
        <Time keyref="this:time1"/>
        <Time>
          <content index="1">
            <!-- xs:any content -->
            <owltime:inXSDDateTime>2003-01-09T12:39:09Z</owltime:inXSDDateTime>
          </content>
        </Time>
        <Atom>
          <op>
            <Rel iri="dc:date"/>
          </op>
          <!-- Dublin Core date -->
            <Time>
                <Data xsi:type="xs:dateTime">2011-03-21T10:00:00-04:00</Data>
            </Time>
        </Atom>

      <!-- interval -->
        <Interval>
            <Time>
                <Ind>t1</Ind>
            </Time>
            <Time>
                <Ind>t2</Ind>
            </Time>
        </Interval>
        <Interval>
            <Var>Y</Var>
        </Interval>
        <Interval>
            <Time>
                <Var>Y1</Var>
            </Time>
            <Time>
                <Var>Y2</Var>
            </Time>
        </Interval>

      <!-- different interval algebra definitions -->

      <!-- [t2,t3] during [t1,4] -->
            <During>
                <Interval>
                    <Time>
                        <Ind>t2</Ind>
                    </Time>
                    <Time>
                        <Ind>t3</Ind>
                    </Time>
                </Interval>
                <Interval>
                    <Time>
                        <Ind>t1</Ind>
                    </Time>
                    <Time>
                        <Ind>t4</Ind>
                    </Time>
                </Interval>
            </During>

      <!-- [t1,t3] overlaps with [t2,t4] -->
            <Overlaps>
                <Interval>
                    <Time>
                        <Ind>t1</Ind>
                    </Time>
                    <Time>
                        <Ind>t3</Ind>
                    </Time>
                </Interval>
                <Interval>
                    <Time>
                        <Ind>t2</Ind>
                    </Time>
                    <Time>
                        <Ind>t4</Ind>
                    </Time>
                </Interval>
            </Overlaps>

      <!-- [t1,t2] starts [t1,t4] -->
            <Starts>
                <Interval>
                    <Time>
                        <Ind>t1</Ind>
                    </Time>
                    <Time>
                        <Ind>t2</Ind>
                    </Time>
                </Interval>
                <Interval>
                    <Time>
                        <Ind>t1</Ind>
                    </Time>
                    <Time>
                        <Ind>t4</Ind>
                    </Time>
                </Interval>
            </Starts>

      <!-- [t1,t2] precedes [t3,t4] -->
            <Precedes>
                <Interval>
                    <Time>
                        <Ind>t1</Ind>
                    </Time>
                    <Time>
                        <Ind>t2</Ind>
                    </Time>
                </Interval>
                <Interval>
                    <Time>
                        <Ind>t3</Ind>
                    </Time>
                    <Time>
                        <Ind>t4</Ind>
                    </Time>
                </Interval>
            </Precedes>

      <!-- [t3,t4] succeeds [t1,t2] -->
            <Succeeds>
                <Interval>
                    <Time>
                        <Ind>t1</Ind>
                    </Time>
                    <Time>
                        <Ind>t2</Ind>
                    </Time>
                </Interval>
                <Interval>
                    <Time>
                        <Ind>t3</Ind>
                    </Time>
                    <Time>
                        <Ind>t4</Ind>
                    </Time>
                </Interval>
            </Succeeds>

      <!-- [t1,t2] meets [t2,t4] -->
            <Meets>
                <Interval>
                    <Time>
                        <Ind>t1</Ind>
                    </Time>
                    <Time>
                        <Ind>t2</Ind>
                    </Time>
                </Interval>
                <Interval>
                    <Time>
                        <Ind>t2</Ind>
                    </Time>
                    <Time>
                        <Ind>t4</Ind>
                    </Time>
                </Interval>
            </Meets>

      <!-- [t1,t2] equals [t1,t2] -->
            <Equal type="rulemm:AllenOperator" style=":deep-equality">
              <left>
                <Interval>
                    <Time>
                        <Ind>t1</Ind>
                    </Time>
                    <Time>
                        <Ind>t2</Ind>
                    </Time>
                </Interval>
              </left>
              <right>
                <Interval>
                    <Time>
                        <Ind>t1</Ind>
                    </Time>
                    <Time>
                        <Ind>t2</Ind>
                    </Time>
                </Interval>
              </right>
            </Equal>

      <!-- [t1,t4] finishes [t3,t4] -->
            <Finishes>
                <Interval>
                    <Time>
                        <Ind>t1</Ind>
                    </Time>
                    <Time>
                        <Ind>t4</Ind>
                    </Time>
                </Interval>
                <Interval>
                    <Time>
                        <Ind>t3</Ind>
                    </Time>
                    <Time>
                        <Ind>t4</Ind>
                    </Time>
                </Interval>
            </Finishes>


    </And>
  </if>
  <then>
    <!-- rule head -->
    <Time>
        <Ind>t2</Ind>
    </Time>
  </then>
</Rule>
