<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="http://consumer.ruleml.org/1.03/nvdl/fipa-consumer-ruleml.nvdl" type="application/xml" schematypens="http://purl.oclc.org/dsdl/nvdl/ns/structure/1.0"?>
<!-- This is an illustrative example of the embedding of Consumer RuleML rule inside a FIPA-ACL envelope, not an actual message from RuleML.-->
<fipa-message act="inform">
  <sender>
    <agent-identifier>
      <name id="RuleML"/>
    </agent-identifier>
  </sender>
  <receiver>
    <agent-identifier>
      <name id="Tara-Athan"/>
    </agent-identifier>
  </receiver>
  <content xmlns:ruleml="http://ruleml.org/spec" xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ex="http://example.org/"
    xmlns:swrlb="http://www.w3.org/2003/11/swrlb">
    <!-- An Axiomatization of Interval Time by J.Allen and P.Hayes
         http://ijcai.org/Past%20Proceedings/IJCAI-85-VOL1/PDF/101.pdf
         rendered in Consumer RuleML 1.03.
    -->
    <ruleml:And style="rulemm:FOL">

      <!-- M3': Nonmaximal on right only
        EXISTS k (i:k) and NOT EXISTS j (j:i) -->
      <ruleml:Forall>
        <ruleml:Var type="rulemm:LeftMaximalInterval">i</ruleml:Var>
        <ruleml:And>
          <ruleml:Exists>
            <ruleml:Var>k</ruleml:Var>
            <ruleml:Meets>
              <ruleml:Var>i</ruleml:Var>
              <ruleml:Var>k</ruleml:Var>
            </ruleml:Meets>
          </ruleml:Exists>
          <ruleml:Neg>
            <ruleml:Exists>
              <ruleml:Var>j</ruleml:Var>
              <ruleml:Meets>
                <ruleml:Var>j</ruleml:Var>
                <ruleml:Var>i</ruleml:Var>
              </ruleml:Meets>
            </ruleml:Exists>
          </ruleml:Neg>
        </ruleml:And>
      </ruleml:Forall>

      <ruleml:Forall>
        <!-- M4'': Closure under ordered union 
          LeftMaximal(i) AND i:j:n  IMPLIES
                 EXISTS LeftMaximal(k) {
                      m:k:n
        -->
        <ruleml:Var type="rulemm:LeftMaximalInterval">i</ruleml:Var>
        <ruleml:Implies closure="universal">
          <ruleml:And>
            <ruleml:Meets>
              <ruleml:Var>i</ruleml:Var>
              <ruleml:Var>j</ruleml:Var>
            </ruleml:Meets>
            <ruleml:Meets>
              <ruleml:Var>j</ruleml:Var>
              <ruleml:Var>n</ruleml:Var>
            </ruleml:Meets>
          </ruleml:And>
          <ruleml:Exists>
            <ruleml:Var type="rulemm:LeftMaximalInterval">k</ruleml:Var>
            <ruleml:And>
              <ruleml:Meets>
                <ruleml:Var>m</ruleml:Var>
                <ruleml:Var>k</ruleml:Var>
              </ruleml:Meets>
              <ruleml:Meets>
                <ruleml:Var>k</ruleml:Var>
                <ruleml:Var>n</ruleml:Var>
              </ruleml:Meets>
            </ruleml:And>
          </ruleml:Exists>
        </ruleml:Implies>
      </ruleml:Forall>

      <ruleml:Forall>
        <!-- M5': Uniqueness
          LeftMaximal(j) and LeftMaximal(k) and
          j:l AND k:l IMPLIES j=k -->
        <ruleml:Var type="rulemm:LeftMaximalInterval">j</ruleml:Var>
        <ruleml:Var type="rulemm:LeftMaximalInterval">k</ruleml:Var>
        <ruleml:Implies>
          <ruleml:And>
            <ruleml:Meets>
              <ruleml:Var>j</ruleml:Var>
              <ruleml:Var>l</ruleml:Var>
            </ruleml:Meets>
            <ruleml:Meets>
              <ruleml:Var>k</ruleml:Var>
              <ruleml:Var>l</ruleml:Var>
            </ruleml:Meets>
          </ruleml:And>
          <ruleml:Equal>
            <ruleml:Var>j</ruleml:Var>
            <ruleml:Var>k</ruleml:Var>
          </ruleml:Equal>
        </ruleml:Implies>
      </ruleml:Forall>


      <ruleml:Forall closure="universal">
        <!--  LeftMaximal(j) AND j:n IMPLIES
              i STARTS j IFF
              LeftMaximal(i) AND
              EXISTS k  (
                i:k:n 
              )
        -->
        <ruleml:Var type="rulemm:LeftMaximalInterval">i</ruleml:Var>
        <ruleml:Var type="rulemm:LeftMaximalInterval">j</ruleml:Var>
        <ruleml:Implies>
          <ruleml:Meets>
            <ruleml:Var>j</ruleml:Var>
            <ruleml:Var>n</ruleml:Var>
          </ruleml:Meets>
          <ruleml:Equivalent closure="universal">
            <ruleml:Starts>
              <ruleml:Var>i</ruleml:Var>
              <ruleml:Var>j</ruleml:Var>
            </ruleml:Starts>
            <ruleml:Exists>
              <ruleml:Var>k</ruleml:Var>
              <ruleml:Meets>
                <ruleml:Var>i</ruleml:Var>
                <ruleml:Var>k</ruleml:Var>
              </ruleml:Meets>
            </ruleml:Exists>
          </ruleml:Equivalent>
        </ruleml:Implies>
      </ruleml:Forall>

      <ruleml:Forall closure="universal">
        <ruleml:Var type="rulemm:LeftMaximalInterval">j</ruleml:Var>
        <ruleml:Implies>
          <ruleml:Meets>
            <ruleml:Var>j</ruleml:Var>
            <ruleml:Var>n</ruleml:Var>
          </ruleml:Meets>
          <ruleml:Equivalent closure="universal">
            <!--  LeftMaximal(j) AND j:n IMPLIES
              i FINISHES j IFF
              EXISTS k n  (
                k:i:n 
              )
        -->
            <ruleml:Finishes>
              <ruleml:Var>i</ruleml:Var>
              <ruleml:Var>j</ruleml:Var>
            </ruleml:Finishes>
            <ruleml:Exists>
              <ruleml:Var>k</ruleml:Var>
              <ruleml:And>
                <ruleml:Meets>
                  <ruleml:Var>k</ruleml:Var>
                  <ruleml:Var>i</ruleml:Var>
                </ruleml:Meets>
                <ruleml:Meets>
                  <ruleml:Var>k</ruleml:Var>
                  <ruleml:Var>n</ruleml:Var>
                </ruleml:Meets>
              </ruleml:And>
            </ruleml:Exists>
          </ruleml:Equivalent>
        </ruleml:Implies>
      </ruleml:Forall>

      <ruleml:Forall closure="universal">
        <ruleml:Var type="rulemm:LeftMaximalInterval">i</ruleml:Var>
        <ruleml:Implies>
          <ruleml:Meets>
            <ruleml:Var>j</ruleml:Var>
            <ruleml:Var>q</ruleml:Var>
          </ruleml:Meets>
          <ruleml:Equivalent closure="universal">
            <!--  LeftMaximal(i) AND j:q IMPLIES
              i OVERLAPS j IFF
              Nonmaximal(j) AND
              EXISTS k LeftMaximal(m) n  (
                m:n:p:q AND
                i:p AND
                m:j
              )
        -->
            <ruleml:Overlaps>
              <ruleml:Var>i</ruleml:Var>
              <ruleml:Var>j</ruleml:Var>
            </ruleml:Overlaps>
            <ruleml:Exists>
              <ruleml:Var>k</ruleml:Var>
              <ruleml:Var type="rulemm:LeftMaximalInterval">m</ruleml:Var>
              <ruleml:Var>n</ruleml:Var>
              <ruleml:And>
                <ruleml:Meets>
                  <ruleml:Var>m</ruleml:Var>
                  <ruleml:Var>n</ruleml:Var>
                </ruleml:Meets>
                <ruleml:Meets>
                  <ruleml:Var>n</ruleml:Var>
                  <ruleml:Var>p</ruleml:Var>
                </ruleml:Meets>
                <ruleml:Meets>
                  <ruleml:Var>p</ruleml:Var>
                  <ruleml:Var>q</ruleml:Var>
                </ruleml:Meets>
                <ruleml:Meets>
                  <ruleml:Var>i</ruleml:Var>
                  <ruleml:Var>p</ruleml:Var>
                </ruleml:Meets>
                <ruleml:Meets>
                  <ruleml:Var>m</ruleml:Var>
                  <ruleml:Var>j</ruleml:Var>
                </ruleml:Meets>
                <ruleml:Meets>
                  <ruleml:Var>k</ruleml:Var>
                  <ruleml:Var>n</ruleml:Var>
                </ruleml:Meets>
              </ruleml:And>
            </ruleml:Exists>
          </ruleml:Equivalent>
        </ruleml:Implies>
      </ruleml:Forall>

      <ruleml:Forall closure="universal">
        <ruleml:Var type="rulemm:LeftMaximalInterval">j</ruleml:Var>
        <ruleml:Implies closure="universal">
          <ruleml:Meets>
            <ruleml:Var>j</ruleml:Var>
            <ruleml:Var>p</ruleml:Var>
          </ruleml:Meets>
          <ruleml:Equivalent closure="universal">
            <!-- LeftMaximal(j) and j:p IMPLIES
            i DURING J IFF
              EXISTS m n (
                m:i:n:p 
              )
        -->
            <ruleml:During>
              <ruleml:Var>i</ruleml:Var>
              <ruleml:Var>j</ruleml:Var>
            </ruleml:During>
            <ruleml:Exists>
              <ruleml:Var type="rulemm:LeftMaximalInterval">m</ruleml:Var>
              <ruleml:Var>n</ruleml:Var>
              <ruleml:And>
                <ruleml:Meets>
                  <ruleml:Var>m</ruleml:Var>
                  <ruleml:Var>i</ruleml:Var>
                </ruleml:Meets>
                <ruleml:Meets>
                  <ruleml:Var>i</ruleml:Var>
                  <ruleml:Var>n</ruleml:Var>
                </ruleml:Meets>
                <ruleml:Meets>
                  <ruleml:Var>n</ruleml:Var>
                  <ruleml:Var>p</ruleml:Var>
                </ruleml:Meets>
              </ruleml:And>
            </ruleml:Exists>
          </ruleml:Equivalent>
        </ruleml:Implies>
      </ruleml:Forall>


    </ruleml:And>
  </content>
</fipa-message>
